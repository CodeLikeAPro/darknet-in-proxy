<IfModule mod_rewrite.c>
   Options +FollowSymLinks
   RewriteEngine On
   RewriteBase /

   # Forcing HTTPS
   RewriteCond %{HTTPS} !=on [OR]
   RewriteCond %{SERVER_PORT} 80
   RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R=301,L]

   RewriteCond %{HTTP_HOST} ^([a-zA-Z0-9-]+).hiddenservice.net
   RewriteCond %{HTTP_HOST} !www.hiddenservice.net
   RewriteRule ^(.*)$ index.php?proxy_path=$1 [L]
</IfModule>
